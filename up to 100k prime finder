#include <stdio.h>
main ()
    {
        int a, b;
        for (a=2; a<=100000; b++)
        {
            for(b=2; b<=a; b++)
            {
                    if(a%b==0)
                    {
                        break;
                    }
            }
            if (a==b)
            {
                printf("%d is a prime number.\n", a);
            }
        }
    }
